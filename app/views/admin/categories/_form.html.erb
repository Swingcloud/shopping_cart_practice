<%= form_for [:admin, @category] do |f| %>
	<div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>
	<div class="form-group">
		<%= f.label :description %>
		<%= f.text_field :description %>
	</div>
	<div class="form-group">
		<%= f.label :enabled %>
		<%= f.check_box :enabled %>
	</div>
	<div class="form-actions">
		<%= f.submit nil, class: 'btn btn-primary' %>
	</div>
<% end %>